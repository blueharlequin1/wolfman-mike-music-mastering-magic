{
  "name": "monster-castle-laboratory",
  "version": "1.0.0",
  "private": true,
  "description": "Wolfman Mike\u0027s Music Mastering Magic \u2014 Monster Castle Laboratory",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "install:all": "npm install && cd server && pip install -r requirements.txt && cd ../client && npm install",
    "start:server": "cd server && flask run --port=5000",
    "start:client": "cd client && npm start",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
    "build:client": "cd client && npm run build",
    "zip": "zip -r monster-castle-laboratory.zip . -x \"node_modules/**\" -x \"**/venv/**\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blueharlequin1/wolfman-mike-music-mastering-magic.git"
  },
  "author": "Wolfman Mike \u0026 Monster Castle Laboratory",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
